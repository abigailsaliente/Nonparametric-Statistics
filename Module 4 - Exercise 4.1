library(EnvStats)
before <- c(10500,8870,12300,10510,5570,9150,11980,6740,7340,13400,12200,
            10570,9880,12100,9000,11800,10500)
after <- c(12600,10660,11890,14630,8580,10115,14350,6900,8890,16540,11300,
           13330,9990,14050,9500,12450,13450)
signTest(before, after, mu=0, alternative = "less",
         conf.level = 0.095, paired= TRUE)

